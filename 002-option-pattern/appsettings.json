{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },  "AllowedHosts": "*",
  "DatabaseOptions": {
    "ConnectionString": "Server=localhost;Database=MyDatabase;User Id=myuser;Password=mypassword;",
    "TimeoutSeconds": 30,
    "EnableRetry": true,
    "MaxRetryAttempts": 3
  },
  "EmailOptions": {
    "SmtpHost": "smtp.example.com",
    "SmtpPort": 587,
    "Username": "myuser",
    "Password": "mypassword",
    "EnableSsl": true,
    "FromEmail": "noreply@example.com",
    "FromName": "Example App"
  }
}
